    "graphql-request": "6.1.0",
    "lodash": "^4.17.21",
    "lodash.merge": "^4.6.2",
    "next": "^14.0.4",
    "postgres": "^3.4.3",
    "plotly.js": "^2.27.1",
    "react": "^18.2.0",
    "react-day-picker": "^8.9.1",
