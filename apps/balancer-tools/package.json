{
  "name": "@bleu/balancer-tools",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "node pre-build.js && next build",
    "build:prepare:gql": "cd ../../ && pnpm run -C=packages/gql graphql:update-types",
    "build:prepare:wagmi": "pnpm run wagmi",
    "build:prepare": "pnpm npm-run-all -l --parallel 'build:prepare:gql'",
    "dev": "node pre-build.js && pnpm npm-run-all -l --parallel dev:**",
    "dev:next": "next dev",
    "dev:gyro-eclp-api": "cd ../gyro-eclp-api && poetry run dev",
    "start": "next start",
    "wagmi": "wagmi generate",
    "wagmi:watch": "wagmi generate --watch",
    "lint": "eslint '**/*.{ts,tsx}'",
    "lint:fix": "eslint '**/*.{ts,tsx}' --fix",
    "test:jest": "pnpm exec jest",
    "test:watch": "pnpm exec jest --watchAll --verbose"
  },
  "dependencies": {
    "@bleu-balancer-tools/gql": "workspace:*",
