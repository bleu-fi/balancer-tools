    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "copy-to-clipboard": "^3.3.3",
    "date-fns": "^2.30.0",
    "downshift": "^8.2.3",
    "fathom-client": "^3.6.0",
    "gql": "^1.1.2",
    "graphql-request": "6.1.0",
    "graphql-tag": "^2.12.6",
